<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options" 
                     xmlns:wpfoly="clr-namespace:WpfOly">
    <wpfoly:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    <wpfoly:KeyToStaticResourceConverter x:Key="KeyToStaticResourceConverter"/>
    <wpfoly:ProvinceTypeToBrushConverter x:Key="ProvinceTypeToBrushConverter"/>
    <wpfoly:SmallerThanConverter x:Key="SmallerThanConverter"/>
    <SolidColorBrush x:Key="forest" Color="ForestGreen" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="ocean" Color="SkyBlue" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="desert" Color="SandyBrown" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="plain" Color="Yellow" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="mountain" Color="Crimson" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="swamp" Color="DimGray" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="tunnel" Color="DarkKhaki" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="chamber" Color="DarkKhaki" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="fill" Color="Khaki" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="hades" Color="Goldenrod" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="cloud" Color="AntiqueWhite" PresentationOptions:Freeze="True" />
    <SolidColorBrush x:Key="underground" Color="Goldenrod" PresentationOptions:Freeze="True" />

    <DrawingBrush x:Key="forest_visited" Stretch="Fill">
        <DrawingBrush.Drawing>
            <GeometryDrawing Brush="{StaticResource forest}">
                <GeometryDrawing.Geometry>
                    <RectangleGeometry Rect="0,0,100,100"/>
                </GeometryDrawing.Geometry>
                <GeometryDrawing.Pen>
                    <Pen Brush="Black" Thickness="2"/>
                </GeometryDrawing.Pen>
            </GeometryDrawing>
        </DrawingBrush.Drawing>  
    </DrawingBrush>
